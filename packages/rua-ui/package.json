{
  "name": "@rua/ui",
  "version": "0.1.0",
  "description": "Pre-built UI components for Rua extensions",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./styles": "./src/styles/index.css",
    "./styles/global.css": "./src/styles/global.css",
    "./styles/command.css": "./src/styles/command.css"
  },
  "files": [
    "dist",
    "src/styles"
  ],
  "scripts": {
    "build": "vite build && tsc --emitDeclarationOnly --declaration --outDir dist",
    "dev": "vite build --watch",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@tanstack/react-virtual": "^3.0.1",
    "date-fns": "^4.1.0",
    "fuse.js": "^7.1.0",
    "motion": "^12.23.24",
    "pinyin-match": "^1.2.10",
    "react-markdown": "^10.1.0",
    "lucide-react": "^0.548.0"
  },
  "peerDependencies": {
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "rollup-plugin-visualizer": "^6.0.5"
  }
}
